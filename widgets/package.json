{
  "name": "widgets",
  "version": "0.1.5",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"src/**/*.{js,mjs,cjs,ts,mts,jsx,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "build": "NODE_ENV=production rollup -c --bundleConfigAsCjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --quiet",
    "serve-storybook": "serve storybook-static -L -n -l tcp://0.0.0.0:$PORT",
    "run-test-storybook": "test-storybook --browsers firefox chromium webkit --url=$STORYBOOK_URL",
    "run-test-units": "TZ=UTC jest --coverage --setupFiles ./setupFile.ts",
    "test": "yarn run-test-storybook && yarn run-test-units"
  },
  "packageManager": "yarn@4.1.0",
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript-graphql-request": "6.2.0",
    "@jest/globals": "29.7.0",
    "@playwright/test": "1.42.1",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/blocks": "7.6.17",
    "@storybook/react": "7.6.17",
    "@storybook/react-webpack5": "7.6.17",
    "@storybook/test": "7.6.17",
    "@storybook/test-runner": "0.16.0",
    "@testing-library/react": "14.2.1",
    "@types/eslint": "^8",
    "@types/jest-image-snapshot": "^6",
    "@types/react": "18.2.61",
    "@types/react-dom": "^18",
    "@types/react-test-renderer": "^18",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "eslint": "8.57.0",
    "eslint-kit": "10.18.0",
    "eslint-plugin-storybook": "0.8.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-image-snapshot": "6.4.0",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "rollup": "4.12.0",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "serve": "14.2.1",
    "storybook": "7.6.17",
    "ts-jest": "29.1.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@tanstack/react-query": "5.24.2",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "styled-components": "6.1.8"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
